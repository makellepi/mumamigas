

<% if @user.matches.nil? %>
  <%= No matched Mumamigas %>
<% else %>
  <% @friends.each do |friend| %>
    <% if friend.friend_status.include?("accepted")||friend.friend_status.include?("declined") %>
      <ul> Your friend request to <%= friend.user.first_name %> is <%= friend.friend_status %></ul>
    <% else %>
      <ul> Your friend request to <%= friend.user.first_name %> is pending</ul>
    <% end %>
  <% end %>
<% end %>



