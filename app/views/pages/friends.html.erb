
<div class="container">
<% if @user.matches.nil? %>
  <%= No matched Mumamigas %>
<% else %>
 <p> Your current Mumamigas: </p>
  <% @friends.each do |friend| %>
    <% if friend.friend_status.include?("accepted")%>
      <ul><%= image_tag friend.user.photo.url, class: "style_image"  %>
       <%= friend.user.first_name %></ul>
      <% end %>
    <% end %>
<% end %>
</div>



